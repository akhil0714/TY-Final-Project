<%- include("../partials/navbar.ejs") %>

  <main class="mt-5 pt-4">
    <div class="container-fluid">
      <div class="row">
        <div class="col-md-12 fw-bold fs-3 mt-2 text-center">Dashboard</div>
      </div>
      <table class="table text-center">
        <thead>
          <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
            <th scope="col">Email</th>
            <th scope="col">Edit</th>
          </tr>
        </thead>
        <tbody>
          <% user.forEach(function(users, index) { %>
            <tr>
              <th scope="row">
                <%= index + 1 %>
              </th>
              <td>
                <%= users.name %>
              </td>
              <td>
                <%= users.email %>
              </td>
              <td><a href="/manageuser/delete/<%= users._id %>" class="btn btn-danger">DELETE</a></td>
            </tr>
            <% }) %>
        </tbody>
      </table>
    </div>
  </main>

  <%- include("../partials/script.ejs") %>